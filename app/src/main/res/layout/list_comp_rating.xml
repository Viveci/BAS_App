<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="None"
        android:id="@+id/list_comp_rating_label"
        android:textColor="#666666"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_centerHorizontal="true"
        android:gravity="center_vertical|center_horizontal" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/list_comp_rating_star5"
        android:src="@drawable/ic_bluestar_empty"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/list_comp_rating_star4"
        android:src="@drawable/ic_bluestar_empty"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/list_comp_rating_star5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/list_comp_rating_star3"
        android:src="@drawable/ic_bluestar_empty"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/list_comp_rating_star4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/list_comp_rating_star2"
        android:src="@drawable/ic_bluestar_empty"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/list_comp_rating_star3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:id="@+id/list_comp_rating_star1"
        android:src="@drawable/ic_bluestar_empty"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/list_comp_rating_star2" />

</RelativeLayout>